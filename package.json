{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun src/index.html",
    "bundle": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='PUBLIC_URL*'",
    "start": "NODE_ENV=production bun src/index.html",
    "check:types": "bun tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@huggingface/transformers": "^3.7.3",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@xenova/transformers": "^2.17.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/bun": "latest"
  }
}
